{
  "name": "metacloudify/corebundle",
  "description": "Operaciones para servicios en la nube",
  "type": "symfony-bundle",
  "authors": [
    {
      "name": "ElUtku",
      "email": "masandres2000@gmail.com",
      "homepage": "https://github.com/ElUtku"
    }
  ],
  "minimum-stability": "stable",
  "prefer-stable": true,
  "keywords": [
    "cloud",
    "metadata",
    "meta",
    "file_explorer",
    "file",
    "explorer"
  ],
  "require": {
    "php": ">=8.2.11",
    "league/flysystem": "^3.23.0",
    "doctrine/annotations": "^2.0",
    "doctrine/dbal": "^3",
    "doctrine/doctrine-bundle": "^2.10",
    "doctrine/doctrine-migrations-bundle": "^3.2",
    "doctrine/orm": "^2.16",
    "symfony/monolog-bundle": "^3.10.0"
  },
  "autoload": {
    "psr-4": {
      "MetaCloudify\\CoreBundle\\": "src/"
    }
  },
  "config": {
    "sort-packages": true
  },
  "conflict": {
    "php": "8.4.2"
  },
  "license": "MIT",
  "repositories": {
    "private-packagist": {
      "type": "composer",
      "url": "https://repo.packagist.com/elutku/"
    }
  },
  "scripts": {
    "install": [
      "MetaCloudify\\CoreBundle\\Resources\\config\\CustomInstallerScript::run"
    ]
  }
}
