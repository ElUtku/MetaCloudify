{
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=8.2.11",
        "ext-ctype": "*",
        "ext-curl": "*",
        "ext-ftp": "*",
        "ext-iconv": "*",
        "doctrine/annotations": "^2.0",
        "doctrine/dbal": "^3",
        "doctrine/doctrine-bundle": "^2.10",
        "doctrine/doctrine-migrations-bundle": "^3.2",
        "doctrine/orm": "^2.16",
        "google/apiclient": "^2.15",
        "guzzlehttp/guzzle": "^7.8",
        "justus/flysystem-onedrive": "^2.0",
        "league/flysystem": "^3.23.0",
        "league/flysystem-ftp": "3.0",
        "league/flysystem-sftp-v3": "3.0",
        "league/flysystem-webdav": "^3.0",
        "sabre/dav": ">=4.6.0",
        "league/oauth2-client": "^2.7",
        "league/oauth2-google": "^4.0",
        "masbug/flysystem-google-drive-ext": "v2.2.2",
        "microsoft/microsoft-graph": "^1.110.0",
        "phpdocumentor/reflection-docblock": "^5.3",
        "phpstan/phpdoc-parser": "^1.24",
        "sensio/framework-extra-bundle": "^6.2",
        "shitware-ltd/flysystem-msgraph": "^1.2.0",
        "stevenmaguire/oauth2-microsoft": "^2.2",
        "symfony/asset": "5.4.*",
        "symfony/console": "5.4.*",
        "symfony/doctrine-messenger": "5.4.*",
        "symfony/dotenv": "5.4.*",
        "symfony/expression-language": "5.4.*",
        "symfony/flex": "^1.17|^2",
        "symfony/form": "5.4.*",
        "symfony/http-client": "5.4.*",
        "symfony/intl": "5.4.*",
        "symfony/mailer": "5.4.*",
        "symfony/mime": "5.4.*",
        "symfony/monolog-bundle": "v3.10.0",
        "symfony/notifier": "5.4.*",
        "symfony/process": "5.4.*",
        "symfony/property-access": "5.4.*",
        "symfony/property-info": "5.4.*",
        "symfony/runtime": "5.4.*",
        "symfony/security-bundle": "5.4.*",
        "symfony/serializer": "5.4.*",
        "symfony/string": "5.4.*",
        "symfony/translation": "5.4.*",
        "symfony/twig-bundle": "5.4.*",
        "symfony/validator": "5.4.*",
        "symfony/web-link": "5.4.*",
        "symfony/yaml": "5.4.*",
        "twig/extra-bundle": "^2.12|^3.0",
        "twig/twig": "^2.12|^3.0",
        "vlucas/phpdotenv": "^5.6",
        "ext-dom": "*",
        "ext-libxml": "*",
        "ext-xmlreader": "*"
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "php-http/discovery": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/",
            "MetaCloudify\\Core\\": "bundles/MetaCloudify/Core/src",
            "MetaCloudify\\OneDrive\\": "bundles/MetaCloudify/OneDrive/src/",
            "MetaCloudify\\OwnCloud\\": "bundles/MetaCloudify/OwnCloud/src/",
            "MetaCloudify\\GoogleDrive\\": "bundles/MetaCloudify/GoogleDrive/src/",
            "MetaCloudify\\Ftp\\": "bundles/MetaCloudify/Ftp/src/",
            "MetaCloudify\\_home\\": "bundles/MetaCloudify/_home/src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.4.*"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "symfony/browser-kit": "5.4.*",
        "symfony/css-selector": "5.4.*",
        "symfony/debug-bundle": "5.4.*",
        "symfony/maker-bundle": "^1.50",
        "symfony/phpunit-bridge": "^6.3",
        "symfony/stopwatch": "5.4.*",
        "symfony/web-profiler-bundle": "5.4.*"
    }
}
